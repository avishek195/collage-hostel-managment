  <main class="table">
    <section class="table__header">
      <h1>Booking Table</h1>
    </section>
    <section class="table__body">

      <table>
        <thead>
          <tr>
            <th> Student id </th>
            <th> Enrollment id </th>
            <th> Room No </th>
            <th> Phone No </th>
            <th> Date</th>
          </tr>
        </thead>
        <tbody>
          <% for(data of initialStateAdmin.data){ %>


            <tr>
              <td>
                <%=data.name %>
              </td>
              <td>
                <%=data.E_no %>
              </td>
              <td>
                <%=data.room.roomId %>
              </td>
              <td>
                <%=data.ph_no %>
              </td>
              <td>
                <%=data.room.bookingDate %>
              </td>

              <% } %>
                <!-- <tr>
                <td>
                  <%=data.name %>
                </td>
                <td>
                  <%=data.ph_no %>
                </td>


                



        </tbody>
      </table>
    </section>
